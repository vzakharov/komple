{
  "name": "Komple",
  "description": "Autocomplete with large language models on (almost) any web page",
  "version": "0.1",
  "permissions": [
      "tabs","<all_urls>","webRequest","storage"
  ],
  "content_scripts": [
      {
      "matches": [
          "http://*/*",
          "https://*/*"
          ],
      "js": ["settings.js", "komple.js", "ui.js", "scrape.js"],
      "run_at": "document_end"
      }
  ], 
  "manifest_version": 2
}